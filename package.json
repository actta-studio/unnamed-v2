{
  "name": "unnamed-v2",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev\" \"npm run watch\" \"npm run browser-sync\"",
    "dev": "shopify theme dev",
    "watch": "nodemon -e js,ts,html,scss,css --ignore assets --exec 'npm run build'",
    "build": "vite build",
    "browser-sync": "browser-sync start --files 'assets/*.*' --no-open --proxy 'http://127.0.0.1:9292'"
  },
  "keywords": [],
