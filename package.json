  "license": "ISC",
  "description": "",
  "devDependencies": {
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "lenis": "^1.3.8"
  }
}
